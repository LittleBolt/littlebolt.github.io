html {
	-webkit-text-size-adjust: 70%; /* Prevent font scaling in landscape */
}

a
{
    -webkit-transition-duration: 0.4s; /* Safari */
    transition-duration: 0.4s;;
}

body {
    overflow-x: hidden;
	color:#555555;
    text-decoration:none;
    font-family:"arial";
	font-weight: 700;
    background-size:100%,100%;
	background-color:#e9edf2;
	height: 100%;
}

a:link
{
    color:#1971c3;
    text-decoration: none;
}

a:visited
{
    color:#1971c3;
    text-decoration: none;
}

a:hover 
{
    color:#2395ff;
    text-decoration: none;
}

a.menu:link
{
    color:#d7d7d7;
    text-decoration: none;
}

a.menu:visited
{
    color:#d7d7d7;
    text-decoration: none;
}
a.menu:hover 
{
    color:#ffffff;
    text-decoration: none;
}

a.menuon:link
{
