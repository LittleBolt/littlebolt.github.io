.ponieschat {
	font-size: 14px;
	color: #FFFFFF;
	background: rgba(51,51,51,0.8);
	position: fixed;
	margin: 0;
	padding: 5px 9px 5px 9px;
	max-Width: 500px;
	text-Align: center;
	border-Radius: 5px;
	-Moz-Border-Radius: 5px;
	width: auto;
	height: auto;
}
.ponieschat:after 
{
content: '';
position: absolute;
border-style: solid;
border-width: 6px 6px 0;
border-color: rgba(51,51,51,0.8) transparent;
display: block;
margin: 0;
width: 0;
bottom: -6px;
left: 43%;
}

